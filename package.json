{
  "author": "virtualpatterns.com <code@virtualpatterns.com> (http://www.virtualpatterns.com)",
  "ava": {
    "failFast": true,
    "files": [
      "release/test/**/*.test.js"
    ],
    "nodeArguments": [
      "--no-warnings",
      "--unhandled-rejections=strict"
    ],
    "require": [
      "@virtualpatterns/mablung-source-map-support/install"
    ],
    "verbose": true
  },
  "babel": {
    "presets": [
      [
        "@virtualpatterns/babel-preset-mablung-makefile",
        {
          "header": {
            "exclude": [
              "source/sandbox/**/*",
              "source/test/**/*"
            ]
          }
        }
      ]
    ]
  },
  "c8": {
    "all": true,
    "exclude": [
      "release/header",
      "release/sandbox"
    ],
    "reporter": [
      "html"
    ],
    "src": "release"
  },
  "description": "TBD",
  "eslintConfig": {
    "extends": "@virtualpatterns/eslint-config-mablung-makefile"
  },
  "exports": {
    ".": "./release/index.cjs",
    "./create-plugin": "./release/library/create-plugin.cjs",
    "./visitor": "./release/library/visitor.cjs",
    "./visitor-error": "./release/library/error/visitor-error.cjs",
    "./package.json": "./package.json"
  },
  "files": [
    "release/library",
    "release/index.cjs"
  ],
  "license": "GPL-3.0+",
  "main": "release/index.cjs",
  "name": "@virtualpatterns/babel-plugin-mablung",
  "repository": {
    "type": "git",
    "url": "https://github.com/virtualpatterns/babel-plugin-mablung"
  },
  "type": "module",
  "version": "0.0.3",
  "dependencies": {
    "change-case": "^4.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@virtualpatterns/babel-preset-mablung-makefile": "0.0.11",
    "@virtualpatterns/eslint-config-mablung-makefile": "0.0.11",
    "@virtualpatterns/mablung-check-dependency": "0.0.5",
    "@virtualpatterns/mablung-makefile": "0.0.21",
    "@virtualpatterns/mablung-source-map-support": "0.0.9",
    "ava": "^3.15.0"
  }
}
